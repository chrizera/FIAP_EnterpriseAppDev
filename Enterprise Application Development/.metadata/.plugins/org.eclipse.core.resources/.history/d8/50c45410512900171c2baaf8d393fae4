package br.com.fiap.entity;

import javax.persistence.Table;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;

@Entity
@Table(name = "T_DEPENDENTE")
@SequenceGenerator(allocationSize = 0, name = "seqDependente", sequenceName = "SQ_T_DEPENDENTE")
public class Dependente {

	@Id
	@Column(name = "CD_DEPENDENTE")
	@GeneratedValue(generator = "seqDependente", strategy = GenerationType.SEQUENCE)
	private int codigo;
	
	private String nome;
}
